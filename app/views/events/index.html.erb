<h1>Listing events</h1>
 
<table>
  <tr>
    <th>Creator</th>
    <th>description</th>
    <th></th>
  </tr>
 
  <% @events.each do |event| %>
    <tr>
      <td><%= event.user.name %></td>
      <td><%= event.description %></td>
      <td><%= link_to 'Show', event_path(event) %></td>
    </tr>
  <% end %>
</table>